class Duck:
    def __init__(self, **kwargs):
        self._color=kwargs.get('color','white')
    def quack(self):
        print('Qaaaak')
    
    def walk(self):
        print("walk like a duck")
    def get_color(self):
        return self._color
def main():
    donald=Duck()
    #donald._color='Blue'
    print(donald.get_color())

    
if __name__ == '__main__':main()
    