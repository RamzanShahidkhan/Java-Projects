def main():
    s='this is s string'
    for i,c in enumerate(s):
        if c=='s':print("index {} is an s".format(i))
        
    fh = open('lines.txt')
    for index, line in enumerate(fh.readlines()):
        print(index,line, end='')

if __name__ == "__main__":main()
